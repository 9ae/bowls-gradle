    </style>

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="BillLabel">
        <item name="android:textSize">@dimen/large_font</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="BillSubLabel">
        <item name="android:textSize">@dimen/xsmall_font</item>
        <item name="android:textColor">@color/lime</item>
    </style>

    <style name="BillValue">
        <item name="android:textSize">@dimen/med_font</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="NumberButton">
        <item name="android:minHeight">@dimen/button_size</item>
        <item name="android:minWidth">@dimen/button_size</item>
        <item name="android:background">@color/num_btn_colors</item>
        <item name="android:textColor">@color/num_btn_text_colors</item>
        <item name="android:layout_margin">@dimen/number_buttons_margin</item>
        <item name="android:textSize">25sp</item>
    </style>

    <style name="NumberPadEnterButton">
        <item name="android:background">@color/num_btn_colors</item>
        <item name="android:textColor">@color/num_btn_text_colors</item>
        <item name="android:layout_margin">@dimen/number_buttons_margin</item>
        <item name="android:textSize">30sp</item>
    </style>

    <style name="SplitButtons">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/split_text</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="ToastLabel">
        <item name="android:textColor">@color/grapefruit</item>
    </style>

    <style name="ToastValue">
        <item name="android:textColor">@color/lime</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingLeft">@dimen/number_buttons_margin</item>
